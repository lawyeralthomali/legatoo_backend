# Environment variables for NO-ML mode
# Copy this to your .env file or set these variables

# Disable ML embeddings completely (prevents OOM crashes)
DISABLE_ML_EMBEDDINGS=true

# Alternative: Force specific model
# EMBEDDING_MODEL=no_ml

# Memory optimization settings
EMBEDDING_BATCH_SIZE=2
EMBEDDING_MAX_SEQUENCE_LENGTH=256
EMBEDDING_CACHE_SIZE=50
